#include <stdio.h>
#include <string.h>
//#include <math.h>
//void f(int *b);
int main(int argc, const char * argv[]) {
    // insert code here...
    // TODO Auto-generated method stub
    char word[100];
    int  i;
    int n=1;
    do
    {
        char c='.';
        scanf("%s",word);
        i=(int)strlen(word);
        if(word[i-1]==c)
        {if(i>1)
            {
               printf("%d\n",i-1);
            }
             
            n=0;
        }
        else{
            printf("%d ",i);
        }
    }while(n);
    return 0;
}
